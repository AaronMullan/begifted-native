{
  "expo": {
    "name": "begifted",
    "slug": "begifted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/begifted-logo.png",
    "scheme": "begifted",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.begifted.app",
      "supportsTablet": true,
      "infoPlist": {
        "NSContactsUsageDescription": "This app would like to access your contacts to help you import gift recipients.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["READ_CONTACTS"]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/begifted-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "16c0ca90-545c-4ff3-be91-227aae28dbe5"
      }
    },
    "owner": "aaronmullan"
  }
}
